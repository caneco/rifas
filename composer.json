{
    "name": "caneco/rifas",
    "description": "",
    "keywords": ["framework", "laravel", "console", "zero"],
    "homepage": "http://laravel-zero.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitor Caneco",
            "email": "vitorcaneco@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/database": "5.5.*",
        "illuminate/filesystem": "5.5.*",
        "laravel-zero/framework": "4.0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["rifas"]
}
